using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.ComponentModel.DataAnnotations;
using System.ComponentModel.DataAnnotations.Schema;
using System.Linq;
using System.Web;
using System.Web.Mvc;

namespace ClubMembership.Models
{
    [Table("MemberShipMaster")]
    public class MemberShipMaster
    {
        [Key]
        [DatabaseGenerated(DatabaseGeneratedOption.Identity)]
        public int MemberID { get; set; }

        [Required]
        [StringLength(128)]
        public string UIserID { get; set; }

        [StringLength(10)]
        public string RegstrId { get; set; }

        [Required]
        public int MemberNo { get; set; }

        [Required]
        [StringLength(25)]
        public string MemberDNo { get; set; }

        [Required]
        public DateTime Member_Reg_Date { get; set; }

        [Required]
        [StringLength(100)]
        public string Member_Name { get; set; }

        [Required]
        public int Gender { get; set; }

        [Required]
        [DisplayFormat(DataFormatString = "{0:yyyy-MM-dd}", ApplyFormatInEditMode = true)]
        public DateTime Member_DOB { get; set; }

        [Required]
        [StringLength(15)]
        public string Member_Age { get; set; }

        [Required]
        public int BldGID { get; set; }

        [Required]
        [StringLength(15)]
        public string Member_Mobile_No { get; set; }

        [StringLength(15)]
        public string Member_WhatsApp_No { get; set; }

        [StringLength(15)]
        public string Member_Landline_No { get; set; }

        [Required]
        [StringLength(100)]
        [EmailAddress]
        public string Member_EmailID { get; set; }

        [Required]
        [StringLength(100)]
        public string Member_Addr1 { get; set; }

        [StringLength(100)]
        public string Member_Addr2 { get; set; }

        [StringLength(100)]
        public string Member_City { get; set; }

        [Required]
        public int StateID { get; set; }

        [Required]
        [StringLength(10)]
        public string Member_Pincode { get; set; }

        [Required]
        [StringLength(100)]
        public string Member_Country { get; set; }

        [Required]
        [StringLength(100)]
        public string Member_Per_Addr1 { get; set; }

        [StringLength(100)]
        public string Member_Per_Addr2 { get; set; }

        [StringLength(100)]
        public string Member_Per_City { get; set; }

        [Required]
        public int PStateID { get; set; }

        [Required]
        [StringLength(10)]
        public string Member_Per_Pincode { get; set; }

        [Required]
        [StringLength(100)]
        public string Member_Per_Country { get; set; }

        public string Member_Photo_Path { get; set; }

        [Required]
        [StringLength(100)]
        public string CreatedBy { get; set; }

        [Required]
        public DateTime CreatedDateTime { get; set; }

        [StringLength(100)]
        public string ModifiedBy { get; set; }

        public DateTime? ModifiedDateTime { get; set; }

        [Required]
        public int DispStatus { get; set; }

        [Required]
        public DateTime Member_Sdate { get; set; }

        [Required]
        public DateTime Member_Edate { get; set; }

        [StringLength(100)]
        public string Spouse_Name { get; set; }

        [Required]
        public DateTime Spouse_DOB { get; set; }

        [Required]
        public DateTime Date_Of_Marriage { get; set; }

        [Required]
        public int Total_Children { get; set; }

        public string Family_Photo_Path { get; set; }

        [Required]
        [StringLength(100)]
        public string Business_Location { get; set; }

        [StringLength(100)]
        public string Business_Name { get; set; }

        [Required]
        [StringLength(100)]
        public string Occupation { get; set; }
    }
}